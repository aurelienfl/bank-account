<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC "-//Puppy Crawl//DTD Suppressions 1.1//EN" "http://www.puppycrawl.com/dtds/suppressions_1_1.dtd">
<!--
    ///////////////////////////////////////////////
    Framework classes files and others classes which are not scanned by checkstyle
    //////////////////////////////////////////////  -->
<suppressions>
	<suppress files=".*Local\.java" checks="[a-zA-Z0-9]*"/>
	<suppress files=".*LocalHome\.java" checks="[a-zA-Z0-9]*"/>
        <suppress files=".*MdbBean\.java" checks="[a-zA-Z0-9]*"/>
	<suppress files=".*Remote\.java" checks="[a-zA-Z0-9]*"/>
	<suppress files=".*RemoteHome\.java" checks="[a-zA-Z0-9]*"/>
</suppressions>
